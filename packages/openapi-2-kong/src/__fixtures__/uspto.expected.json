{
  "services": [
    {
      "url": "https://developer.uspto.gov/ds-api",
      "name": "USPTO_Data_Set_API",
      "plugins": [],
      "routes": [
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "list_data_sets",
          "paths": ["/$"]
        },
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "list_searchable_fields",
          "paths": ["\/(?<dataset>\\S+)\/(?<version>\\S+)\/fields$"]
        },
        {
          "methods": ["POST"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "perform_search",
          "paths": ["\/(?<dataset>\\S+)\/(?<version>\\S+)\/records$"]
        }
      ],
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"]
    }
  ],
  "upstreams": [
    {
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
      "name": "USPTO_Data_Set_API",
      "targets": [{"target": "developer.uspto.gov:443"}]
    }
  ],
  "_format_version": "1.1"
}